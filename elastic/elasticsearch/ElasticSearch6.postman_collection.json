				"body": {
					"mode": "raw",
					"raw": "{\n  \"order\": 0,\n  \"template\": \"odigeo_4_*\",\n  \"settings\": {\n    \"index.number_of_replicas\": \"1\",\n    \"index.number_of_shards\": \"2\",\n      \"index.refresh_interval\": \"30s\"\n  },\n  \"mappings\": {\n    \"calendarsearch\": {\n      \"_id\": {\n        \"path\": \"id\",\n        \"index\": \"not_analyzed\"\n      },\n      \"_timestamp\": {\n        \"path\": \"timestamp\",\n        \"enabled\": true\n      },\n      \"_all\": {\n        \"enabled\": false\n      },\n      \"properties\": {\n        \"website\": {\n          \"index\": \"not_analyzed\",\n          \"type\": \"string\"\n        },\n        \"triptype\": {\n          \"index\": \"not_analyzed\",\n          \"type\": \"string\"\n        },\n        \"origin\": {\n          \"type\": \"long\"\n        },\n        \"destination\": {\n          \"type\": \"long\"\n        },\n        \"mktPortal\": {\n          \"index\": \"not_analyzed\",\n          \"type\": \"string\"\n        },\n        \"calendarSearchFareItineraryResultDTOs\": {\n          \"type\": \"nested\",\n          \"properties\": {\n            \"apparentPrices\": {\n              \"properties\": {\n                \"appartentPrice\": {\n                  \"properties\": {\n                    \"amount\": {\n                      \"index\": \"no\",\n                      \"type\": \"long\"\n                    },\n                    \"currency\": {\n                      \"index\": \"no\",\n                      \"type\": \"string\"\n                    }\n                  }\n                },\n                \"apparentPrice\": {\n                  \"properties\": {\n                    \"amount\": {\n                      \"index\": \"no\",\n                      \"type\": \"double\"\n                    },\n                    \"currency\": {\n                      \"index\": \"no\",\n                      \"type\": \"string\"\n                    }\n                  }\n                },\n                \"apparentPriceCriteria\": {\n                  \"index\": \"not_analyzed\",\n                  \"type\": \"string\"\n                }\n              }\n            },\n            \"fee\": {\n              \"properties\": {\n                \"amount\": {\n                  \"index\": \"no\",\n                  \"type\": \"long\"\n                },\n                \"currency\": {\n                  \"index\": \"no\",\n                  \"type\": \"string\"\n                }\n              }\n            },\n            \"inboundSegments\": {\n              \"properties\": {\n                \"carrier\": {\n                  \"index\": \"no\",\n                  \"type\": \"string\"\n                },\n                \"arrDate\": {\n                  \"format\": \"dateOptionalTime\",\n                  \"index\": \"no\",\n                  \"type\": \"date\"\n                },\n                \"stopOvers\": {\n                  \"index\": \"no\",\n                  \"type\": \"long\"\n                },\n                \"depGeoNodeId\": {\n                  \"type\": \"long\"\n                },\n                \"arrGeoNodeId\": {\n                  \"index\": \"not_analyzed\",\n                  \"type\": \"long\"\n                },\n                \"depDate\": {\n                  \"format\": \"dateOptionalTime\",\n                  \"index\": \"no\",\n                  \"type\": \"date\"\n                }\n              }\n            },\n            \"providerPrice\": {\n              \"properties\": {\n                \"amount\": {\n                  \"index\": \"no\",\n                  \"type\": \"long\"\n                },\n                \"currency\": {\n                  \"index\": \"no\",\n                  \"type\": \"string\"\n                }\n              }\n            },\n            \"outboundSegments\": {\n              \"properties\": {\n                \"carrier\": {\n                  \"index\": \"no\",\n                  \"type\": \"string\"\n                },\n                \"arrDate\": {\n                  \"format\": \"dateOptionalTime\",\n                  \"index\": \"no\",\n                  \"type\": \"date\"\n                },\n                \"stopOvers\": {\n                  \"index\": \"no\",\n                  \"type\": \"long\"\n                },\n                \"depGeoNodeId\": {\n                  \"index\": \"not_analyzed\",\n                  \"type\": \"long\"\n                },\n                \"arrGeoNodeId\": {\n                  \"type\": \"long\"\n                },\n                \"depDate\": {\n                  \"format\": \"dateOptionalTime\",\n                  \"index\": \"no\",\n                  \"type\": \"date\"\n                }\n              }\n            },\n            \"providers\": {\n              \"index\": \"no\",\n              \"type\": \"string\"\n            }\n          }\n        },\n        \"interface\": {\n          \"index\": \"not_analyzed\",\n          \"type\": \"string\"\n        },\n        \"arrivalDate\": {\n          \"format\": \"dateOptionalTime\",\n          \"type\": \"date\"\n        },\n        \"itineraryDestination\": {\n          \"type\": \"long\"\n        },\n        \"datesKey\": {\n          \"index\": \"not_analyzed\",\n          \"type\": \"string\"\n        },\n        \"isMktPortalChangingPrice\": {\n          \"type\": \"boolean\"\n        },\n        \"numPassengers\": {\n          \"type\": \"long\"\n        },\n        \"departureDate\": {\n          \"format\": \"dateOptionalTime\",\n          \"type\": \"date\"\n        },\n        \"id\": {\n          \"index\": \"no\",\n          \"type\": \"string\"\n        },\n        \"isResident\": {\n          \"type\": \"boolean\"\n        },\n        \"itineraryOrigin\": {\n          \"type\": \"long\"\n        },\n        \"timestamp\": {\n          \"format\": \"dateOptionalTime\",\n          \"index\": \"no\",\n          \"type\": \"date\"